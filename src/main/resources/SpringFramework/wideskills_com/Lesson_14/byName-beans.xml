<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
    <bean id="allotedTo" class="spring_framework.wideskills_com.lesson_14.xml.byName.Student">
        <property name="name" value="Student A" />
    </bean>
    <bean id="room" class="spring_framework.wideskills_com.lesson_14.xml.byName.Room" autowire="byName">
        <property name="roomNumber" value="R-101" />
    </bean>
</beans>